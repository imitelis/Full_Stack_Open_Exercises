    "@react-native/assets-registry": "^0.74.0",
    "dotenv": "^16.3.1",
    "expo": "~46.0.21",
    "expo-linking": "^6.2.2",
    "expo-status-bar": "~1.4.0",
    "formik": "^2.4.5",
    "graphql": "^15.8.0",
    "react": "18.2.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.9",
    "react-native-web": "~0.18.7",
    "react-router-native": "^6.16.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
